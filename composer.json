{
	"name": "officialxviid/favicon",
	"type": "library",
	"description": "Simple PHP library to help developers 🐘 do better on-page Favicon optimization",
	"keywords": [
		"favicon", 
		"favicon optimization", 
		"php7",
		"schema.org",
		"sitemaps", 
		"sitemap.xml", 
		"sitemap index",
		"favicon tags"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "XVIID",
			"email": "support@xviid.net",
			"homepage": "https://xviid.net",
			"role": "Developer"
		}
	],
	"require":  {
		"php": ">=7.2",
		"ext-xml": "*",
		"ext-curl": "*",
		"ext-json": "*"
	},
	"autoload": {
		"psr-4": {
			"OfficalXVIID\\Favicon\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4":{
			"Tests\\OfficalXVIID\\Favicon\\": "tests/"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
